{
  "crons": [
    {
      "path": "/api/cron/echo",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/social-sync",
      "schedule": "*/5 * * * *"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/api/openapi.json",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/llms.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain; charset=utf-8" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/.well-known/airc",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/openapi.json", "destination": "/openapi.json" },
    { "source": "/api/presence/heartbeat", "destination": "/api/presence/heartbeat.js" },
    { "source": "/api/presence/who", "destination": "/api/presence/who.js" },
    { "source": "/api/messages/send", "destination": "/api/messages/send.js" },
    { "source": "/api/messages/inbox", "destination": "/api/messages/inbox.js" },
    { "source": "/api/messages/thread", "destination": "/api/messages/thread.js" },
    { "source": "/api/presence", "destination": "/api/presence.js" },
    { "source": "/api/messages", "destination": "/api/messages.js" },
    { "source": "/api/profile", "destination": "/api/profile.js" },
    { "source": "/api/users", "destination": "/api/users.js" },
    { "source": "/api/consent", "destination": "/api/consent.js" },
    { "source": "/api/friends", "destination": "/api/friends.js" },
    { "source": "/api/stats", "destination": "/api/stats.js" },
    { "source": "/api/gigabrain/ingest", "destination": "/api/gigabrain/ingest.js" },
    { "source": "/api/gigabrain/query", "destination": "/api/gigabrain/query.js" },
    { "source": "/api/gigabrain/backfill", "destination": "/api/gigabrain/backfill.js" },
    { "source": "/api/board", "destination": "/api/board.js" },
    { "source": "/api/cron/echo", "destination": "/api/cron/echo.js" },
    { "source": "/api/discord-bridge", "destination": "/api/discord-bridge.js" },
    { "source": "/api/social", "destination": "/api/social/index.js" },
    { "source": "/api/cron/social-sync", "destination": "/api/cron/social-sync.js" }
  ]
}
